	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#757180"
			}
		},
		{
			"settings": {
				"background": "#24222E",
				"foreground": "#E0DCE8"
			}
		},
		{
			"name": "Foreground",
			"scope": [
				"keyword.operator.class",
				"constant.other",
				"source.php.embedded.line"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#B7B3C7"
			}
		},
		{
			"name": "Variable",
			"scope": "variable.other.property",
			"settings": {
				"foreground": "#E36687"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#F8786B"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"meta.require",
				"support.function.any-method",
				"entity.name.function"
			],
			"settings": {
				"foreground": "#70B7E6"
			}
		},
		{
			"name": "Tags",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#E36687"
			}
		},
		{
			"name": "Methods",
